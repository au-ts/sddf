					interrupts = <0x00 0x5a 0x04>;
					clocks = <0x02 0xb9 0x02 0xb8 0x02 0x00 0x02 0x00>;
					clock-names = "bus", "mclk1", "mclk2", "mclk3";
					dmas = <0x11 0x08 0x02 0x00 0x11 0x09 0x02 0x00>;
