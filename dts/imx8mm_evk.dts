					status = "disabled";
				};

				sai@30060000 {
					#sound-dai-cells = <0x00>;
					compatible = "fsl,imx8mm-sai", "fsl,imx8mq-sai";
					reg = <0x30060000 0x10000>;
					interrupts = <0x00 0x5a 0x04>;
					clocks = <0x02 0xbb 0x02 0xba 0x02 0x00 0x02 0x00>;
