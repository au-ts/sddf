            <map mr="tx_buffer_data_region_cli0" vaddr="0x3_200_000" perms="rw" cached="true"  />
            <map mr="tx_buffer_data_region_cli1" vaddr="0x3_400_000" perms="rw" cached="true"  />
        </protection_domain>

        <protection_domain name="client0" priority="97" id="6" cpu="2">
