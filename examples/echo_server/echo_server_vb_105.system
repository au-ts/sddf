            <map mr="eth0" vaddr="0x2_000_000" perms="rw" cached="true"/>

            <map mr="hw_rx_ring_buffer" vaddr="0x2200000" perms="rw" cached="true" setvar_vaddr="hw_ring_buffer_vaddr" />
            <map mr="hw_tx_ring_buffer" vaddr="0x2202000" perms="rw" cached="true" setvar_vaddr="hw_ring_buffer_vaddr" />

            <map mr="rx_free_drv" vaddr="0x2_400_000" perms="rw" cached="true" setvar_vaddr="rx_free" />
            <map mr="rx_used_drv" vaddr="0x2_600_000" perms="rw" cached="true" setvar_vaddr="rx_used" />
            <map mr="tx_free_drv" vaddr="0x2_800_000" perms="rw" cached="true" setvar_vaddr="tx_free" />
