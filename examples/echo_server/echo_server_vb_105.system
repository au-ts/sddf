            <map mr="tx_buffer_data_region_arp" vaddr="0x2_800_000" perms="rw" cached="true" setvar_vaddr="tx_buffer_data_region" />
            <map mr="rx_buffer_data_region" vaddr="0x2_a00_000" perms="r" cached="true" setvar_vaddr="rx_buffer_data_region" />
        </protection_domain>

        <protection_domain name="serial" priority="254" pp="true" id="10" passive="true" cpu="0">
          <program_image path="serial.elf" />
          <ioport ioport="0" start="0x3f8" end="0x400" />
        </protection_domain>

        <protection_domain name="timer" priority="253" pp="true" id="9" passive="true" cpu="0">
            <program_image path="timer.elf" />
            <map mr="timer" vaddr="0x500_000_000" perms="rw" cached="true" />
            <irq irq="0" id="0" type="msi" handle="0" pci_bus="0" pci_dev="0" pci_func="0" />
